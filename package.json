{
  "name": "@kareem-3del/nestjs-aramex",
  "version": "1.0.3",
  "description": "NestJS library for Aramex shipping API integration with tracking and shipping services",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "prepublishOnly": "npm run build",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "test:integration": "jest --config ./test/jest-integration.json",
    "test:integration:watch": "jest --config ./test/jest-integration.json --watch",
    "test:integration:auth": "jest --config ./test/jest-integration.json --testPathPattern=authentication.integration.spec.ts",
    "test:integration:shipping": "jest --config ./test/jest-integration.json --testPathPattern=shipping.integration.spec.ts",
    "test:integration:tracking": "jest --config ./test/jest-integration.json --testPathPattern=tracking.integration.spec.ts",
    "test:integration:errors": "jest --config ./test/jest-integration.json --testPathPattern=error-handling.integration.spec.ts",
    "test:integration:performance": "jest --config ./test/jest-integration.json --testPathPattern=performance.integration.spec.ts",
    "test:all": "npm run test && npm run test:e2e && npm run test:integration",
    "test:comprehensive": "npm run test:coverage && npm run test:e2e && npm run test:integration",
    "test:ci": "npm run test:coverage && npm run test:e2e",
    "validate:env": "node scripts/validate-env.js",
    "validate:test-env": "node scripts/test-environment.js",
    "security:audit": "node scripts/security-audit.js",
    "security:check": "npm audit && npm run security:audit",
    "setup:local": "npm run validate:env && npm install && npm run build",
    "aramex:test": "node test-aramex.ts",
    "aramex:health": "node -e \"require('./test-aramex.ts').testAramexAPI().then(() => process.exit(0)).catch(() => process.exit(1))\""
  },
  "keywords": [
    "nestjs",
    "aramex",
    "shipping",
    "tracking",
    "logistics",
    "api",
    "soap",
    "kareem-3del",
    "delivery"
  ],
  "author": "",
  "license": "MIT",
  "peerDependencies": {
    "@nestjs/common": "^10.0.0",
    "@nestjs/core": "^10.0.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1"
  },
  "dependencies": {
    "@nestjs/axios": "^3.0.2",
    "axios": "^1.6.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "dotenv": "^17.2.2",
    "fast-xml-parser": "^5.2.5",
    "soap": "^1.4.1"
  },
  "devDependencies": {
    "@nestjs/common": "^10.0.0",
    "@nestjs/core": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@types/jest": "^29.5.0",
    "@types/node": "^20.0.0",
    "@types/soap": "^0.18.0",
    "jest": "^29.5.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "ts-jest": "^29.1.0",
    "typescript": "^5.0.0"
  }
}
